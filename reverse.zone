;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	freedomtech.lan. root.freedomtech.lan. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	freedomtech.lan.
104	IN	PTR	raspberrypi.freedomtech.lan.
